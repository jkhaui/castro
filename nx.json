{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "generators": {
    "@nx/js:library": {
      "buildable": true
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "executor": "@nx/js:tsc"
    },
    "dev": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore"
      ]
    },
    "test": {}
  }
}
